{
  "name": "blog-posts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thenem/blog-posts.git"
  },
  "scripts": {
    "prettier": "prettier",
    "embedme": "embedme content/**/*.md",
    "prettier:base": "yarn run prettier \"content/**/*.{js,json,scss,md,ts,html,component.html}\"",
    "prettier:write": "yarn run prettier:base --write",
    "prettier:check": "yarn run prettier:base --list-different",
    "embedme:check": "yarn run embedme --verify",
    "embedme:write": "yarn run embedme",
    "dev-to-git": "dev-to-git"
  },
  "dependencies": {
    "dev-to-git": "^1.4.1",
    "embedme": "^1.22.0",
    "prettier": "^2.2.1"
  }
}
